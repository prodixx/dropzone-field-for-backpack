{
    "name": "prodixx/dropzone-field-for-backpack",
    "description": "Dropzone field for Laravel Backpack",
    "license": "MIT",
    "authors": [
        {
            "name": "Catalin Prodan",
            "email": "catalin.prodan@newpixel.ro",
            "homepage": "https://www.newpixel.ro"
        }
    ],
    "homepage": "https://github.com/prodixx/dropzone-field-for-backpack",
    "keywords": [
        "Laravel", "Backpack", "Backpack for Laravel", "Backpack Addon", "DropzoneFieldForBackpack"
    ],
    "require": {
        "illuminate/support": "~7|~8",
        "backpack/crud": "^4.1.0",
        "intervention/image": "^2.5"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "Prodixx\\DropzoneFieldForBackpack\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Prodixx\\DropzoneFieldForBackpack\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --testdox"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Prodixx\\DropzoneFieldForBackpack\\AddonServiceProvider"
            ]
        }
    }
}
